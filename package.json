{
  "name": "marzban-sdk",
  "version": "1.4.1",
  "license": "MIT",
  "description": "Fully typed client SDK for the Marzban API, supporting both browser and Node.js environments.",
  "private": false,
  "author": "ilmar7786",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "generate:docs": "node ./tools/generateDocs.js",
    "commitlint": "commitlint --edit",
    "prepare": "husky",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "codegen": "kubb generate"
  },
  "keywords": [
    "marzban-sdk",
    "marzban",
    "marzban api",
    "marzban api client",
    "marzban vpn",
    "marzban vpn api",
    "marzban vpn api client",
    "vpn",
    "http-client",
    "api-client",
    "vpn",
    "proxy",
    "shadowsocks",
    "v2ray",
    "xray",
    "trojan",
    "wireguard",
    "vmess",
    "vless",
    "hysteria",
    "tuic",
    "rest-api",
    "http",
    "networking"
  ],
  "dependencies": {
    "axios": "^1.13.2",
    "axios-retry": "^4.5.0",
    "chalk": "^5.6.2",
    "ws": "^8.18.3",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@eslint/js": "^9.39.2",
    "@kubb/cli": "^4.12.15",
    "@kubb/core": "^4.12.15",
    "@kubb/plugin-client": "^4.12.15",
    "@kubb/plugin-oas": "^4.12.15",
    "@kubb/plugin-ts": "^4.12.15",
    "@kubb/plugin-zod": "^4.12.15",
    "@types/node": "^25.0.3",
    "@types/ws": "^8.18.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ilmar7786/marzban-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Ilmar7786/marzban-sdk/issues"
  },
  "homepage": "https://github.com/Ilmar7786/marzban-sdk#readme",
  "files": [
    "dist"
  ]
}
