{
  "name": "marzban-sdk",
  "version": "1.4.2",
  "license": "MIT",
  "description": "Fully typed client SDK for the Marzban API, supporting both browser and Node.js environments.",
  "private": false,
  "author": "ilmar7786",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "rimraf": "rimraf",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "codegen": "openapi-generator-cli generate --generator-key v1",
    "openapi-generator-cli": "openapi-generator-cli",
    "generate:docs": "node ./tools/generateDocs.js",
    "commitlint": "commitlint --edit",
    "prepare": "husky",
    "format": "prettier --write .",
    "lint": "eslint --fix ."
  },
  "keywords": [
    "marzban-sdk",
    "marzban",
    "marzban api",
    "marzban api client",
    "marzban vpn",
    "marzban vpn api",
    "marzban vpn api client",
    "vpn",
    "http-client",
    "api-client",
    "proxy",
    "shadowsocks",
    "v2ray",
    "xray",
    "trojan",
    "wireguard",
    "vmess",
    "vless",
    "hysteria",
    "tuic",
    "rest-api",
    "http",
    "networking"
  ],
  "dependencies": {
    "axios": "^1.12.2",
    "axios-retry": "^4.5.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.36.0",
    "@openapitools/openapi-generator-cli": "^2.24.0",
    "@types/node": "^24.6.0",
    "@types/ws": "^8.18.1",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.45.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ilmar7786/marzban-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Ilmar7786/marzban-sdk/issues"
  },
  "homepage": "https://github.com/Ilmar7786/marzban-sdk#readme",
  "files": [
    "dist"
  ]
}
